{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "ignorePatterns": ["__tests__/"],
  "rules": {
    "max-depth": ["error", 1],
    "no-new": "off",
    "max-lines-per-function": ["error", 10]
  }
}
